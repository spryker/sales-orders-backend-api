{
    "name": "spryker/sales-orders-backend-api",
    "type": "library",
    "description": "SalesOrdersBackendApi module",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "spryker/kernel": "^3.66.0",
        "spryker/sales": "^11.40.0",
        "spryker/sales-orders-backend-api-extension": "^0.2.0",
        "spryker/transfer": "^3.31.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/container": "*",
        "spryker/oms": "*",
        "spryker/propel": "*",
        "spryker/store": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
